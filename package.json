{
  "name": "coc-bibtex",
  "version": "1.2.10",
  "description": "Coc list & completion source for BibTeX",
  "main": "./lib/index.js",
  "files": [
    "lib/"
  ],
  "homepage": "https://github.com/oncomouse/coc-bibtex#readme",
  "bugs": {
    "url": "https://github.com/oncomouse/coc-bibtex/issues",
    "email": "oncomouse@gmail.com"
  },
  "author": "Andrew Pilsch <oncomouse@gmail.com> (https://andrew.pilsch.com)",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "prepare": "webpack"
  },
  "keywords": [
    "coc.nvim",
    "neovim",
    "vim",
    "bibtex"
  ],
  "engines": {
    "coc": "^0.0.72"
  },
  "activationEvents": [
    "onLanguage:pandoc",
    "onLanguage:markdown",
    "onLanguage:latex"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "list.source.bibtex.files": {
          "type": "array",
          "default": [],
          "description": "List of .bib files to read.",
          "items": {
            "type": "string"
          }
        },
        "list.source.bibtex.citation": {
          "type": "object",
          "default": {
            "before": "[@",
            "after": "]"
          },
          "properties": {
            "before": {
              "type": "string",
              "description": "What to place before the citation when inserting citations"
            },
            "after": {
              "type": "string",
              "description": "What to place after the citation when inserting citations"
            }
          }
        },
        "bibtex.enable": {
          "type": "boolean",
          "default": true
        },
        "coc.source.bibtex.shortcut": {
          "type": "string",
          "default": "BIB"
        },
        "coc.source.bibtex.triggerCharacters": {
          "type": "array",
          "default": [
            "@"
          ],
          "items": {
            "type": "string"
          }
        },
        "coc.source.bibtex.filetypes": {
          "type": [
            "array",
            "null"
          ],
          "default": [
            "latex",
            "pandoc",
            "markdown"
          ],
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "parser": "babel-eslint",
    "rules": {
      "no-unused-vars": "warn"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "bibtex-parse-js": "0.0.24",
    "coc.nvim": "0.0.79",
    "eslint": "^7.9.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
